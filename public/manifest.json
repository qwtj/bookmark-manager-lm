{
  "manifest_version": 3,
  "name": "Bookmark Manager Extension",
  "version": "1.0.0",
  "description": "A Chrome extension built with Vite and React.",
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage", "tabs", "bookmarks"]
}
